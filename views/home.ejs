<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>NextGen Developers</title>
  <style>
    /* Base Styles */
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    *::selection {
      background-color: rgb(205, 205, 196);
      color: rgb(255, 251, 251);
    }

    body {
      font-family: 'Helvetica Neue', sans-serif;
      background: #fff;
      color: #1c1c1c;
      line-height: 1.6;
    }

    header {
      background: #f9f9f9;
      padding: 30px 50px;
      display: flex;
      justify-content: space-between;
      align-items: center;
      border-bottom: 1px solid #e0e0e0;
    }

    .logo {
      font-size: 30px;
      font-weight: bold;
      color: #111;
    }

    nav a {
      margin-left: 20px;
      text-decoration: none;
      color: #333;
      font-size: 20px;
      transition: color 0.3s;
    }

    nav a:hover {
      color: #1e88e5;
    }

    .hero {
      text-align: center;
      padding: 100px 20px 60px;
      background: linear-gradient(to right, #e3f2fd, #fff);
    }

    .hero h1 {
      font-size: 55px;
      font-weight: 600;
    }

    .hero p {
      font-size: 23px;
      color: #555;
      margin-top: 20px;
    }

    .hero button {
      margin-top: 30px;
      padding: 14px 32px;
      font-size: 18px;
      background-color: #1e88e5;
      color: white;
      border: none;
      border-radius: 8px;
      cursor: pointer;
      transition: background-color 0.3s;
    }

    .hero button:hover {
      background-color: #1565c0;
    }

    .section {
      padding: 70px 30px;

      margin: auto;
      text-align: center;
    }

    .section h2 {
      font-size: 40px;
      margin-bottom: 28px;
    }

    .cards {
      display: flex;
      flex-wrap: wrap;
      gap: 20px;
      justify-content: center;
      margin-top: 30px;
    }

    .card {
      background: #f4f4f4;
      padding: 40px;
      border-radius: 12px;
      width: 300px;
      transition: all 0.3s;
    }

    .card:hover {
      background: #e0f7fa;
      transform: translateY(-5px);
    }

    footer {
      background: #1c1c1c;
      color: white;
      text-align: center;
      padding: 30px 10px;
    }

    footer a {
      color: #90caf9;
      text-decoration: none;
    }

    @media (max-width: 768px) {
      .hero h1 {
        font-size: 36px;
      }

      .card {
        width: 100%;
      }
    }

    .course-section {
      max-width: 1200px;
      margin: 60px auto;
      padding: 50px;
      background: white;
      border-radius: 12px;
      box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
      animation: fadeIn 1s ease-in;
    }

    @keyframes fadeIn {
      from {
        opacity: 0;
        transform: translateY(30px);
      }

      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    .course-section h2 {
      color: #2c3e50;
      font-size: 2.5rem;
      margin-bottom: 20px;
    }

    .course-section p {
      font-size: 1.4rem;
      line-height: 1.8;
      margin-bottom: 20px;
    }

    .highlight {
      background: #dff0ff;
      padding: 5px 10px;
      border-radius: 6px;
      color: #0056b3;
      font-weight: bold;
    }

    .course-section strong {
      color: #1e88e5;
    }

    @media (max-width: 768px) {
      .course-section {
        padding: 20px;
        margin: 20px;
      }

      .course-section h2 {
        font-size: 1.5rem;
      }
    }

    .std-img {
      width: 100%;
      height: 100%;
      display: flex;
      justify-content: center;
      align-items: center;
      background-size: cover;
      background-position: center;
      border-radius: 50%;
      margin: 20px;

    }

    .cards .card {
      font-size: 20px;
      padding: 20px;
    }

    .admin-login-btn {
      display: inline-block;
      padding: 10px 20px;
      background-color: #333;
      color: white !important;
      text-decoration: none;
      border-radius: 5px;
      margin-top: 20px;
    }

    .admin-login-btn:hover {
      background-color: #555;
    }

    /* WhatsApp Floating Button */
    .whatsapp-float {
      position: fixed;
      width: 60px;
      height: 60px;
      bottom: 20px;
      right: 20px;
      background-color: #25d366;
      color: white;
      border-radius: 50%;
      text-align: center;
      font-size: 30px;
      box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.3);
      z-index: 100;
      display: flex;
      align-items: center;
      justify-content: center;
      transition: transform 0.3s ease;
    }

    .whatsapp-float:hover {
      transform: scale(1.1);
    }

    .whatsapp-float img {
      width: 40px;
      height: 40px;
      margin: 10px;
    }

    /* Hide hamburger on desktop */
    .hamburger {
      display: none;
      font-size: 32px;
      cursor: pointer;
    }

    .mobile-nav {
      position: absolute;
      top: 80px;
      left: 0;
      right: 0;
      background-color: #fff;
      border-top: 1px solid #e0e0e0;
      display: flex;
      flex-direction: column;
      align-items: center;
      padding: 20px 0;
      gap: 18px;
      z-index: 999;
      transition: all 0.3s ease;
    }

    .mobile-nav a {
      color: #333;
      font-size: 20px;
      text-decoration: none;
    }

    .hidden {
      display: none;
    }

    .admin-login-btn a {
      color: white;
    }

    @media (max-width: 768px) {
      nav.desktop-nav {
        display: none;
      }

      .hamburger {
        display: block;
      }
    }

    .faculty-section {
      padding: 60px 20px;
      background-color: #f7f9fc;
      text-align: center;
    }

    .faculty-section h2 {
      font-size: 2.5rem;
      margin-bottom: 40px;
      color: #222;
    }

    .faculty-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
      gap: 30px;
      max-width: 1200px;
      margin: 0 auto;
    }

    .faculty-card {
      background-color: #fff;
      border-radius: 16px;
      padding: 25px 20px;
      box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);
      transition: all 0.3s ease;
    }

    .faculty-card:hover {
      transform: translateY(-8px);
      box-shadow: 0 12px 35px rgba(0, 0, 0, 0.15);
    }

    .faculty-card img {
      width: 100px;
      height: 100px;
      object-fit: cover;
      border-radius: 50%;
      margin-bottom: 15px;
      border: 3px solid #4b7bec;
    }

    .faculty-card h3 {
      font-size: 1.25rem;
      margin-bottom: 5px;
      color: #333;
    }

    .faculty-card .title {
      font-weight: 600;
      font-size: 0.95rem;
      color: #4b7bec;
      margin-bottom: 10px;
    }

    .faculty-card p {
      font-size: 0.9rem;
      color: #555;
    }

    .whatsapp-float {
      position: fixed;
      bottom: 30px;
      right: 20px;
      z-index: 999;
      animation: floatUpDown 1s infinite ease-in-out;
    }

    .whatsapp-float img {
      width: 60px;
      height: 60px;
      transition: transform 0.3s;
    }

    @keyframes floatUpDown {

      0%,
      100% {
        transform: translateY(0);
      }

      50% {
        transform: translateY(-20px);
      }
    }

    iframe {
      width: 100%;
      height: 300px;
      border: 2px solid;
      border-radius: 10px;
    }

